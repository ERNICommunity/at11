{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "object-literal-sort-keys": false,
        "max-line-length": [true, 140],
        "no-console": false,
        "variable-name": { "options": ["allow-leading-underscore"] },
        "trailing-comma": [true, { "multiline": "never", "singleline": "never" }],
        "arrow-parens": false
    }
}
